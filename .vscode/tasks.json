{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: configure (Debug)",
      "type": "shell",
      "command": "cmake",
      "linux": {
        "command": "/home/zxc/Qt/Tools/CMake/bin/cmake",
        "args": [
          "-S", "${workspaceFolder}",
          "-B", "${workspaceFolder}/build/vscode-Debug",
          "-DCMAKE_BUILD_TYPE=Debug",
          "-DCMAKE_PREFIX_PATH=/home/zxc/Qt/6.10.2/gcc_64",
          "-DQt6_DIR=/home/zxc/Qt/6.10.2/gcc_64/lib/cmake/Qt6",
          "-DCMAKE_EXPORT_COMPILE_COMMANDS=ON"
        ]
      },
      "windows": {
        "command": "C:/Qt/Tools/CMake_64/bin/cmake.exe",
        "options": {
          "env": {
            "Path": "C:/Qt/Tools/mingw1310_64/bin;${env:Path}"
          }
        },
        "args": [
          "-S", "${workspaceFolder}",
          "-B", "${workspaceFolder}/build/vscode-Debug",
          "-G", "MinGW Makefiles",
          "-DCMAKE_BUILD_TYPE=Debug",
          "-DCMAKE_EXPORT_COMPILE_COMMANDS=ON",
          "-DCMAKE_PREFIX_PATH=C:/Qt/6.10.2/mingw_64",
          "-DQt6_DIR=C:/Qt/6.10.2/mingw_64/lib/cmake/Qt6",
          "-DCMAKE_MAKE_PROGRAM=C:/Qt/Tools/mingw1310_64/bin/mingw32-make.exe",
          "-DCMAKE_C_COMPILER=C:/Qt/Tools/mingw1310_64/bin/gcc.exe",
          "-DCMAKE_CXX_COMPILER=C:/Qt/Tools/mingw1310_64/bin/g++.exe"
        ]
      },
      "problemMatcher": ["$gcc"],
      "group": "build"
    },
    {
      "label": "CMake: build (Debug)",
      "type": "shell",
      "command": "cmake",
      "args": [
        "--build",
        "${workspaceFolder}/build/vscode-Debug"
      ],
      "linux": {
        "command": "/home/zxc/Qt/Tools/CMake/bin/cmake"
      },
      "windows": {
        "command": "C:/Qt/Tools/CMake_64/bin/cmake.exe",
        "options": {
          "env": {
            "Path": "C:/Qt/Tools/mingw1310_64/bin;${env:Path}"
          }
        }
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "dependsOn": ["CMake: configure (Debug)"]
    },
