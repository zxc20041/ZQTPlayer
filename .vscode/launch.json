{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ZQTPlayer",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "preLaunchTask": "CMake: build (Debug)",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Disable debuginfod to avoid slow symbol downloads",
          "text": "set debuginfod enabled off",
          "ignoreFailures": true
        }
      ],
      "linux": {
        "program": "${workspaceFolder}/build/vscode-Debug/ZQTPlayer",
        "miDebuggerPath": "/usr/bin/gdb",
        "environment": [
          {
            "name": "DEBUGINFOD_URLS",
            "value": ""
          }
        ]
      },
      "windows": {
        "program": "${workspaceFolder}/build/vscode-Debug/ZQTPlayer.exe",
        "miDebuggerPath": "C:/Qt/Tools/mingw1310_64/bin/gdb.exe",
        "environment": [
          {
            "name": "Path",
            "value": "C:/Qt/6.10.2/mingw_64/bin;C:/Qt/Tools/mingw1310_64/bin;C:/ffmpeg/ffmpeg-master-latest-win64-lgpl-shared/bin;${env:Path}"
          },
          {
            "name": "QT_ASSUME_STDERR_HAS_CONSOLE",
            "value": "1"
          }
        ]
      },
      "visualizerFile": "c:\\Users\\zxc\\AppData\\Roaming\\Code\\User\\workspaceStorage\\55ff0febb1a66784f4777492c43388cd\\tonka3000.qtvsctools\\qt.natvis.xml"
    },
